                    <rdf:Description rdf:about="#Class"/>
                    <rdf:Description rdf:about="#EnumerationType"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://evolizer.org/ontologies/seon/2009/06/java.owl#declaresMethod -->

    <owl:ObjectProperty rdf:about="#declaresMethod">
