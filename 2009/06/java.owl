        <rdfs:domain rdf:resource="#Class"/>
        <rdfs:range rdf:resource="#Class"/>
        <rdfs:subPropertyOf rdf:resource="#inhertitsFrom"/>
    </owl:ObjectProperty>
    


    <!-- http://evolizer.org/ontologies/seon/2009/06/java.owl#implementsInterface -->

    <owl:ObjectProperty rdf:about="#implementsInterface">
        <rdfs:range rdf:resource="#Interface"/>
        <rdfs:subPropertyOf rdf:resource="#inhertitsFrom"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Class"/>
                    <rdf:Description rdf:about="#EnumerationType"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://evolizer.org/ontologies/seon/2009/06/java.owl#inhertitsFrom -->

    <owl:ObjectProperty rdf:about="#inhertitsFrom">
        <rdfs:domain>
            <owl:Class>
